{% extends 'base.html.twig' %}
{% block body %}
<div class="container">
  <div class="row ">
    <h1 class="mt-4 mb-3">Bar Paco
        <small>Registro de Usuario</small>
    </h1>
    <div class="row">
    <div class="col-lg-6">
      {{ form_start(form_user) }}
      {{ form_errors(form_user) }} 
    </div>
      <div class="col-lg-6">
        {{ form_label(form_user.email) }}
        {{ form_errors(form_user.email) }}
        {{ form_widget(form_user.email) }} 
      </div>
      <div class="col-lg-6">
          {{ form_widget(form_user) }}
      </div>
      {{ form_end(form_user) }}
    </div>
</div>
</div>
{% endblock %}
{% block javascripts %}
<script type="text/javascript" src="{{ asset ('../src/newImguser.js') }}">
</script>
{% endblock %}